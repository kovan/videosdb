        {
            "type": "chrome",
            "request": "launch",
            "name": "Quasar App: chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/client/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${webRoot}/*"
            }
        }        
        {
            "name": "Django: Tests",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["test", "--noinput"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: sync-wordpress",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--sync-wordpress"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: publish-one",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--publish-one", "CR5HtTsUl5E"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: download-one",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--download-one", "CR5HtTsUl5E"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: download-all",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--download-all"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }

    ]
}