{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "cwd": "${workspaceFolder}/client"
        },
        {
            "name": "Django: Tests",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["test", "--noinput"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: sync-wordpress",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--sync-wordpress"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: publish-one",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--publish-one", "CR5HtTsUl5E"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: download-one",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--download-one", "CR5HtTsUl5E"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Django: download-all",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "args": ["videosdb", "--download-all"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }

    ]
}