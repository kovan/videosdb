{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Backend run check",
      "type": "python",
      "request": "launch",
      "module": "videosdb",
      "pythonArgs": [
        "-W",
        "default"
      ],
      "env": {
        //"DEBUG": "1",
        "LOGLEVEL": "DEBUG",
        "YOUTUBE_API_URL": "http://127.0.0.1:2000/youtube/v3",
        "PYTHONDEVMODE": "1",
        "FIRESTORE_EMULATOR_HOST": "localhost:6001",
        "YOUTUBE_API_KEY": "AIzaSyDM-rEutI1Mr6_b1Uz8tofj2dDlwcOzkjs"
      },
      "args": [
        "-c",
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "yarn generate",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "generate"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "env": {
        "DEBUG": "1",
        "NODE_ENV": "development"
      },
      "cwd": "${workspaceFolder}/frontend",
      "console": "integratedTerminal",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "name": "yarn dev",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "dev"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "name": "yarn build",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "build"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "name": "yarn start",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "start"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "port": 9229,
      "justMyCode": false
    },
  ]
}