{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
      {
      "name": "nuxt generate",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": ["<node_internals>/**"],
      "args": ["generate"],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std"
    },


    {
      "name": "nuxt dev",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": ["<node_internals>/**"],
      "args": ["dev"],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std"
    },
    {
      "name": "nuxt build",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": ["<node_internals>/**"],
      "args": ["build"],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std"
    },
    {
      "name": "nuxt start",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": ["<node_internals>/**"],
      "args": ["start"],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std"
    },    
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Program",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229
    },
    {
      "name": "Django: Tests",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "env": {
        "DEBUG": "1"
      },
      "args": ["test", "-n", "--noinput"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Django: publish-one",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": ["videosdb", "--publish-one", "CR5HtTsUl5E"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend"
    },
    {
      "name": "Django: download-one",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": ["videosdb", "--download-one", "CR5HtTsUl5E"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend"
    },
    {
      "name": "Django custom",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": ["videosdb", "-c"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Django runserver",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": ["runserver"],
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    }
  ]
}
