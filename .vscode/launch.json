{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "yarn generate",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "generate"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "env": {
        "DEBUG": "1"
      },
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "name": "yarn dev",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "dev"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "name": "yarn build",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "build"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "name": "yarn start",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "start"
      ],
      "type": "node",
      "envFile": "${workspaceFolder}/frontend/.env",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "justMyCode": false
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "port": 9229,
      "justMyCode": false
    },
    {
      "name": "Django: Tests",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "django": true,
      "env": {
        //"DEBUG": "1",
        "LOGLEVEL": "INFO",
        "YOUTUBE_API_NO_CACHE": "1",
        "YOUTUBE_API_URL": "http://127.0.0.1:2000/youtube/v3",
        "PYTHONDEVMODE": "1"
      },
      "args": [
        "test",
        "--nomigrations",
        "--noinput",
        "--keepdb"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Django: publish-one",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": [
        "videosdb",
        "--publish-one",
        "CR5HtTsUl5E"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Django: download-one",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": [
        "videosdb",
        "--download-one",
        "CR5HtTsUl5E"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Django: check",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": [
        "videosdb",
        "-c"
      ],
      "env": {
        "LOGLEVEL": "DEBUG",
        "DEBUG": "1"
      },
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Django custom",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": [
        "videosdb",
        "-c"
      ],
      "env": {
        "LOGLEVEL": "DEBUG",
        "DEBUG": "1"
      },
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    },
    {
      "name": "Django runserver",
      "type": "python",
      "request": "launch",
      "program": "manage.py",
      "args": [
        "runserver",
        "--insecure",
        "0.0.0.0:8000"
      ],
      "cwd": "${workspaceFolder}/backend",
      "justMyCode": false
    }
  ]
}