    {
        // Use IntelliSense to learn about possible attributes.
        // Hover to view descriptions of existing attributes.
        // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Backend run check",
                "type": "python",
                "request": "launch",
                "program": "run.py",
                "pythonArgs": [
                    "-W",
                    "default"
                ],
                "env": {
                    "DEBUG": "1",
                    "LOGLEVEL": "DEBUG",
                    "PYTHONDEVMODE": "1",
                    "FIRESTORE_EMULATOR_HOST": "127.0.0.1:46456",
                    "YOUTUBE_API_URL": "http://127.0.0.1:43783/youtube/v3"
                },
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "args": [
                    "-c",
                    "-e"
                ],
                "console": "integratedTerminal",
                "cwd": "${workspaceFolder}/backend",
                "justMyCode": false
            },
            {
                "name": "Python: Debug Tests",
                "type": "python",
                "env": {
                    "DEBUG": "1",
                    "LOGLEVEL": "DEBUG",
                    "PYTHONDEVMODE": "1",
                },
                "request": "launch",
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "program": "${file}",
                "purpose": [
                    "debug-test"
                ],
                "console": "integratedTerminal",
                "justMyCode": false
            },
            {
                "name": "Python: Current File",
                "type": "python",
                "request": "launch",
                "program": "${file}",
                "console": "integratedTerminal",
                "justMyCode": false
            },
            {
                "name": "yarn generate",
                "request": "launch",
                "runtimeExecutable": "yarn",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "args": [
                    "generate"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "env": {
                    "NODE_ENV": "development",
                },
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std",
                "justMyCode": false
            },
            {
                "name": "yarn dev",
                "request": "launch",
                "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "env": {
                    "DEBUG": "1",
                    "NODE_ENV": "development",
                },
                "args": [
                    "dev"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std",
                "justMyCode": false
            },
            {
                "name": "yarn build",
                "request": "launch",
                "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "args": [
                    "build"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/frontend/.env",
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std",
                "justMyCode": false
            },
            {
                "name": "yarn start",
                "request": "launch",
                "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "args": [
                    "start"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/frontend/.env",
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std",
                "justMyCode": false
            },
            {
                "type": "node",
                "request": "attach",
                "name": "Attach Program",
                "console": "integratedTerminal",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "port": 9229,
                "justMyCode": false
            },
        ]
    }
