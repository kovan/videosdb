    {
        // Use IntelliSense to learn about possible attributes.
        // Hover to view descriptions of existing attributes.
        // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Python: Debug Tests",
                "type": "python",
                "pythonArgs": [
                    "-W",
                    "default"
                ],
                "env": {
                    "LOGLEVEL": "DEBUG",
                    "PYTHONDEVMODE": "1",
                },
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "request": "launch",
                "module": "unittest",
                "args": [
                    "-v"
                ],
                "purpose": [
                    "debug-test"
                ],
                "console": "integratedTerminal",
                "cwd": "${workspaceFolder}/backend",
                "justMyCode": true
            },
            {
                "name": "Backend run check",
                "type": "python",
                "request": "launch",
                "program": "run.py",
                "pythonArgs": [
                    "-W",
                    "default"
                ],
                "env": {
                    "LOGLEVEL": "DEBUG",
                    "PYTHONDEVMODE": "1",
                },
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "args": [
                    "-c",
                    "--export-to-emulator-host",
                    "127.0.0.1:46456"
                ],
                "console": "integratedTerminal",
                "cwd": "${workspaceFolder}/backend",
                "justMyCode": true
            },
            {
                "name": "yarn generate",
                "request": "launch",
                "runtimeExecutable": "yarn",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "args": [
                    "generate"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "env": {
                    "NODE_ENV": "development",
                    "FIRESTORE_EMULATOR_HOST": "localhost:45456",
                },
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std"
            },
            {
                "name": "yarn dev",
                "request": "launch",
                "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "env": {
                    "DEBUG": "1",
                    "NODE_ENV": "development",
                },
                "args": [
                    "dev"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/common/env/testing.txt",
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std"
            },
            {
                "name": "yarn build",
                "request": "launch",
                "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "args": [
                    "build"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/frontend/.env",
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std"
            },
            {
                "name": "yarn start",
                "request": "launch",
                "runtimeExecutable": "${workspaceFolder}/frontend/node_modules/.bin/nuxt",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "args": [
                    "start"
                ],
                "type": "node",
                "envFile": "${workspaceFolder}/frontend/.env",
                "cwd": "${workspaceFolder}/frontend",
                "console": "integratedTerminal",
                "outputCapture": "std"
            },
            {
                "type": "node",
                "request": "attach",
                "name": "Attach Program",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "port": 9229
            },
            {
                "name": "Leiningen: Run",
                "type": "clojure",
                "request": "launch",
                "cwd": "${workspaceFolder}/backend/clojure",
                "replType": "clj",
                "projectType": "lein",
                "main": "user",
                "args": [],
                "env": {},
                "console": "integratedTerminal"
            },
        ]
    }