{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "kind": {
            "type": "string"
        },
        "id": {
            "type": "string"
        },
        "snippet": {
            "type": "object",
            "properties": {
                "publishedAt": {},
                "channelTitle": {
                    "type": "string"
                },
                "channelId": {
                    "type": "string"
                },
                "thumbnails": {
                    "type": "object",
                    "properties": {
                        "medium": {
                            "type": "object",
                            "properties": {
                                "height": {
                                    "type": "integer"
                                },
                                "url": {
                                    "type": "string"
                                },
                                "width": {
                                    "type": "integer"
                                }
                            }
                        },
                        "default": {
                            "type": "object",
                            "properties": {
                                "height": {
                                    "type": "integer"
                                },
                                "url": {
                                    "type": "string"
                                },
                                "width": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "description": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "statistics": {},
        "videosdb": {
            "type": "object",
            "properties": {
                "slug": {
                    "type": "string"
                },
                "playlists": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "durationSeconds": {
                    "type": "number"
                },
                "descriptionTrimmed": {
                    "type": "string"
                }
            }
        },
        "etag": {
            "type": "string"
        },
        "contentDetails": {
            "type": "object",
            "properties": {
                "duration": {
                    "type": "string"
                },
                "projection": {
                    "type": "string"
                },
                "definition": {
                    "type": "string"
                },
                "caption": {
                    "type": "string"
                }
            }
        }
    }
}
