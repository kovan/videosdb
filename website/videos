server {

  server_name videos.enlightenment.yoga;

  location / {
      proxy_pass http://localhost:8080;
  }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/videos.enlightenment.yoga/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/videos.enlightenment.yoga/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = videos.enlightenment.yoga) {
        return 301 https://$host$request_uri;
    } # managed by Certbot



  server_name videos.enlightenment.yoga;


  listen 80;
    return 404; # managed by Certbot


}